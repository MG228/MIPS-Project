.data
message: .asciiz  "\nPlease enter a postive interger greater than 25: " #getting user input
error: .asciiz "\nIllegal Number! Please enter a postive number > 25!: " #error message
Name: .asciiz "\n\nFibonacci Sequence\n" #list title
nl: .asciiz "\n" #new line

.text
main:
# asks the user for an interger
li $v0, 4 
la $a0, message
syscall

# getting users number
li $v0, 5
syscall

#storing the result
move $t0, $v0

# check if the number is legal
addi $t1, $zero, 25

slt $t2, $t0, $t1
bne $t2, $zero, printm


printm:
li $v0, 4
la $a0, error
syscall

fib:
beq $t4, $t3, exit # if the number == count it will stop
li $v0, 1 # printing first number
move $a0, $t1
syscall

li $v0, 5
la $a0, nl # Creating new line seporations
syscall




