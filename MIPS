.data
message: .asciiz  "\nPlease enter a postive interger greater than 25: " #getting user input
error: .asciiz "\nIllegal Number! Please enter a postive number > 25!: " #error message

.text
main:
# asks the user for an interger
li $v0, 4 
la $a0, message
syscall

# getting users number
li $v0, 5
syscall

#storing the result
move $t0, $v0

# check if the number is legal
addi $t1, $zero, 25

slt $t2, $t0, $t1
bne $t2, $zero, printm

li $v0, 10
syscall

printm:
li $v0, 4
la $a0, error
syscall

# fibonacci series
sub $sp, $sp, 12 #stacks
sw $t0, 0($sp)
sw $s0, 4($sp)
sw $ra, 8($sp)